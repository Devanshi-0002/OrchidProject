@font-face {
    font-family: 'junctionbold';
    src: url('../fonts/Junction-bold-webfont.eot');
    src: url('../fonts/Junction-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/Junction-bold-webfont.woff2') format('woff2'),
         url('../fonts/Junction-bold-webfont.woff') format('woff'),
         url('../fonts/Junction-bold-webfont.ttf') format('truetype'),
         url('../fonts/Junction-bold-webfont.svg#junctionbold') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'junctionlight';
    src: url('../fonts/Junction-light-webfont.eot');
