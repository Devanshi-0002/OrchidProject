         url('../fonts/Junction-light-webfont.ttf') format('truetype'),
         url('../fonts/Junction-light-webfont.svg#junctionlight') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'junctionregular';
    src: url('../fonts/Junction-regular-webfont.eot');
    src: url('../fonts/Junction-regular-webfont.eot?#iefix') format('embedded-opentype'),
